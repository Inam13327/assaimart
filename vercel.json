{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.js"
    },
    {
      "source": "/admin/login",
      "destination": "/index.html" 
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/admin/login",
      "permanent": false
    }
  ]
}