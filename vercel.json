{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/",
      "status": 302,
      "headers": { "Location": "/admin/login" }
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}