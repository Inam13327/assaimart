{
  "version": 2,
  "rewrites": [
    {
"source":"/api/(.*)",
"destination" : "/admin/login"
    },
    {
      "source": "/api/(.*)",
      "destination": "/lib/api.ts"
    },
    {
      "source": "/admin/login",
      "destination": "/pages/AdminLogin" 
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/admin/login",
      "permanent": false
    }
  ]
}